<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.maiya.model.CollectPic" table="collectPic">
        <id column="Id" name="id">
            <generator class="increment"/>
        </id>
        <property name="collectId" column="collectId"/>
        <property name="picId" column="picId"/>
        <property name="vaild" column="vaild"/>
    </class>
</hibernate-mapping>